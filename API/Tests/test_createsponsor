import requests
import json

# location serving the API
api_host = 'ec2-52-205-128-217.compute-1.amazonaws.com:8080'

try:
    result = requests.post(f'http://{api_host}/createsponsor', json={'name': "testcreate",
      'rate': .75})
    print(result.json())
except:
    print('test failed' + result.json())